{
  "name": "@cloudflare/pages-functions-compiler",
  "version": "0.3.3",
  "description": "",
  "main": "index.ts",
  "directories": {
    "examples": "examples"
  },
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "esbuild cli.ts --format=esm --platform=node --bundle --external:esbuild --external:commander --outfile=bin/plinko.mjs",
    "example:config": "yarn build && ./bin/plinko.mjs -c examples/config-routing/pages.json examples/config-routing",
    "example:filepath": "yarn build && ./bin/plinko.mjs examples/filepath-routing",
    "prepublishOnly": "yarn build",
    "test": "jest"
  },
  "bin": {
    "plinko": "bin/plinko.mjs"
  },
  "files": [
    "bin",
    "templates"
  ],
  "author": "Cina Saffary <cina@cloudflare.com>",
  "license": "ISC",
  "dependencies": {
    "@types/acorn": "^4.0.6",
    "@types/jest": "^27.0.2",
    "acorn": "^8.4.1",
    "acorn-walk": "^8.1.1",
    "commander": "^8.1.0",
    "esbuild": "^0.12.15",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.2.2",
    "path-to-regexp": "^6.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.[tj]sx?$": [
        "esbuild-jest",
        {
          "sourcemap": true,
          "format": "cjs"
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
